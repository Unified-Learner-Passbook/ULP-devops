  <builders>
    <hudson.tasks.Shell>
      <command>rm -rf node_modules
rm -rf package-lock.json
npm i

npm run build --prod --aot --output-hashing=all

cd dist/

tar -cvf build.tar sunbird-rc-ui

cp build.tar /var/lib/jenkins/workspace/ULP/build/DEV-Wallet</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
